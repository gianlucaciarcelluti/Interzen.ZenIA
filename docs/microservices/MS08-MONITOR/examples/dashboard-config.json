{
  "dashboard": {
    "id": null,
    "title": "MS01-Classifier Overview",
    "description": "Dashboard di monitoraggio per MS01-Classifier con metriche chiave, performance e alert",
    "tags": ["ms01-classifier", "production", "overview"],
    "timezone": "UTC",
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "up{service=\"ms01-classifier\"}",
            "legendFormat": "Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {
                    "text": "DOWN",
                    "color": "red"
                  },
                  "1": {
                    "text": "UP",
                    "color": "green"
                  }
                },
                "type": "value"
              }
            ]
          }
        },
        "gridPos": {
          "h": 4,
          "w": 6,
          "x": 0,
          "y": 0
        }
      },
      {
        "id": 2,
        "title": "Request Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"ms01-classifier\"}[5m])",
            "legendFormat": "Requests/sec"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 6,
          "y": 0
        }
      },
      {
        "id": 3,
        "title": "Error Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(http_requests_total{service=\"ms01-classifier\",status=~\"5..\"}[5m]) / rate(http_requests_total{service=\"ms01-classifier\"}[5m]) * 100",
            "legendFormat": "Error Rate %"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 4,
        "title": "Processing Latency",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{service=\"ms01-classifier\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{service=\"ms01-classifier\"}[5m]))",
            "legendFormat": "50th percentile"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 16
        }
      },
      {
        "id": 5,
        "title": "Resource Usage",
        "type": "graph",
        "targets": [
          {
            "expr": "container_cpu_usage_seconds_total{pod=~\"ms01-classifier-.*\"}",
            "legendFormat": "CPU Usage"
          },
          {
            "expr": "container_memory_usage_bytes{pod=~\"ms01-classifier-.*\"}",
            "legendFormat": "Memory Usage"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 0,
          "y": 24
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "templating": {
      "list": [
        {
          "name": "tenant",
          "type": "query",
          "query": "label_values(tenant_id)",
          "label": "Tenant"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "name": "Alerts",
          "datasource": "Prometheus",
          "enable": true,
          "query": "ALERTS{alertstate=\"firing\", service=\"ms01-classifier\"}"
        }
      ]
    },
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "links": [
      {
        "title": "Runbook",
        "type": "link",
        "url": "https://zenia.runbooks/ms01-classifier"
      }
    ]
  },
  "folderId": 1,
  "overwrite": false
}